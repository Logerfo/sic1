{
  "name": "sic1-client",
  "version": "1.2.4",
  "description": "Single-instruction computer programming game (web client)",
  "scripts": {
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "watch:test": "npx mocha --watch --require ts-node/register test/*.spec.ts --watch --watch-extensions ts",
    "build": "parcel build --no-source-maps --public-url ./ index.html",
    "build:dev": "parcel build --no-optimize --public-url ./ index.html",
    "serve": "parcel index.html"
  },
  "source": "index.html",
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "mocha": "6",
    "parcel": "^2.7.0",
    "parcel-namer-hashless": "^1.0.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "preact": "^10.5.7",
    "sic1-server-contract": "../server/contract/dist",
    "sic1-shared": "../shared/dist",
    "sic1asm": "../../lib/dist"
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
